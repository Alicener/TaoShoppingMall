<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/my_background_jianxi"
    android:orientation="vertical" >
    
	<com.sun.shoppingmall.customview.Title
        android:layout_width="match_parent"
    	android:layout_height="50dp" >
        
    </com.sun.shoppingmall.customview.Title>
    
	<View 
	    android:layout_width="match_parent"
    	android:layout_height="1dp" 
    	android:background="@color/my_background_jianxi"
	    />

	<RelativeLayout
	    android:id="@+id/setrela1"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/white" >

		<TextView
		    android:id="@+id/textView1"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:textSize="18dp"
		    android:text="推送通知" />
		
		<View 
		    android:layout_width="match_parent"
	    	android:layout_height="1dp" 
	    	android:background="@color/my_background_jianxi"
	    	android:layout_marginLeft="10dp"
	    	android:layout_alignParentBottom="true"
	    	android:layout_marginRight="10dp"
		    />

	</RelativeLayout>

	<RelativeLayout
	    android:id="@+id/setrela2"
	    android:layout_width="match_parent"
	    android:layout_height="50dp" 
	    android:background="@color/white" >

		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:textSize="18dp"
		    android:text="清除本地缓存" />

	</RelativeLayout>
	
	<RelativeLayout
	    android:id="@+id/setrela3"
	    android:layout_width="match_parent"
	    android:layout_height="50dp" 
	    android:background="@color/white"
	    android:layout_marginTop="10dp" >

		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:textSize="18dp"
		    android:text="关于淘金喜" />

		<ImageView
		    android:layout_width="20dp"
		    android:layout_height="20dp"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:layout_marginRight="20dp"
		    android:src="@drawable/yjt" />
		<View 
		    android:layout_width="match_parent"
	    	android:layout_height="1dp" 
	    	android:background="@color/my_background_jianxi"
	    	android:layout_marginLeft="10dp"
	    	android:layout_alignParentBottom="true"
	    	android:layout_marginRight="10dp"
		    />
	</RelativeLayout>
	
	<RelativeLayout
	    android:id="@+id/setrela4"
	    android:layout_width="match_parent"
	    android:layout_height="50dp"
	    android:background="@color/white" >

	    <TextView
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentLeft="true"
	        android:layout_centerVertical="true"
	        android:layout_marginLeft="10dp"
	        android:text="版本更新"
	        android:textSize="18dp" />

	    <View
	        android:id="@+id/view1"
	        android:layout_width="match_parent"
	        android:layout_height="1dp"
	        android:layout_alignParentBottom="true"
	        android:layout_marginLeft="10dp"
	        android:layout_marginRight="10dp"
	        android:background="@color/my_background_jianxi" />

	    <TextView
	        android:id="@+id/textView2"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignRight="@+id/view1"
	        android:layout_centerVertical="true"
	        android:textColor="#929397"
	        android:text="版本更新v1.5.5" />
	</RelativeLayout>
    
	<RelativeLayout
	    android:id="@+id/setrela5"
	    android:layout_width="match_parent"
	    android:layout_height="50dp" 
	    android:background="@color/white"
	     >

		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:textSize="18dp"
		    android:text="意见反馈" />

		<ImageView
		    android:layout_width="20dp"
		    android:layout_height="20dp"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:layout_marginRight="20dp"
		    android:src="@drawable/yjt" />
		<View 
		    android:layout_width="match_parent"
	    	android:layout_height="1dp" 
	    	android:background="@color/my_background_jianxi"
	    	android:layout_marginLeft="10dp"
	    	android:layout_alignParentBottom="true"
	    	android:layout_marginRight="10dp"
		    />
		
		
	</RelativeLayout>
	<RelativeLayout
	    android:id="@+id/setrela6"
	    android:layout_width="match_parent"
	    android:layout_height="50dp" 
	    android:background="@color/white"
	     >

		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_alignParentLeft="true"
		    android:layout_centerVertical="true"
		    android:layout_marginLeft="10dp"
		    android:textSize="18dp"
		    android:text="去评分" />

		<ImageView
		    android:layout_width="20dp"
		    android:layout_height="20dp"
		    android:layout_alignParentRight="true"
		    android:layout_centerVertical="true"
		    android:layout_marginRight="20dp"
		    android:src="@drawable/yjt" />
		
		
	</RelativeLayout>
	
</LinearLayout>
